{
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "extends": ["eslint:recommended", "plugin:prettier/recommended"],
  "env": {
    "browser": true,
    "es6": true,
    "node": true,
    "jquery": false,
    "commonjs": true
  },
  "rules": {
    "no-console": "off",
    "lines-between-class-members": ["error", "always"]
  },
  "globals": {
    "L": "readonly"
  }
}

